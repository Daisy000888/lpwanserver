    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "prisma": "^1.33.0",
    "speccy": "^0.11.0"
  },
  "engines": {
    "node": ">=8.3.0",
    "npm": ">=5"
  }
