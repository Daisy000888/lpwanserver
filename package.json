{
  "name": "lpwanserver-rest",
  "version": "1.2.1",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "nodemon app/index.js --watch app",
    "package": "node ./bin/package.js",
    "test:unit": "nyc --reporter=lcovonly jest app",
    "test:api": "mocha --opts test/api/mocha.opts",
    "test:e2e": "mocha --opts test/e2e/mocha.opts",
    "test:e2e-https": "mocha --opts test/e2e-https/mocha.opts",
    "clean": "./bin/clean.js",
    "prisma": "cd prisma && prisma",
    "lint": "eslint app/",
    "coverage": "cat ./coverage/lcov.info | codacy-coverage",
    "openapi:lint": "speccy lint docs/openapi/api.yml",
    "openapi:publish": "speccy resolve -o docs/openapi/dist/api.yml docs/openapi/api.yml && cp docs/openapi/dist/api.yml app/api/openapi.yml",
    "openapi:serve": "speccy serve docs/openapi/api.yml"
  },
  "dependencies": {
    "@hapi/joi": "^15.0.2",
    "ajv": "^6.10.0",
    "async": "~2.6.1",
    "bluebird": "^3.5.5",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "connect-ensure-login": "~0.1.1",
    "cors": "^2.8.5",
    "crypto": "~1.0.1",
    "db-migrate": "^0.11.4",
    "dead-leaves": "^1.0.2",
    "debug": "^4.1.0",
    "express": "^4.16.4",
    "express-busboy": "^7.0.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "js-yaml": "^3.13.1",
    "jsmin": "~1.0.1",
    "jsonwebtoken": "^8.4.0",
    "mkdirp": "~0.5.1",
    "morgan": "^1.9.1",
    "object-hash": "^1.3.1",
    "object-sizeof": "^1.3.0",
    "openapi-backend": "^2.3.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prisma-client-lib": "^1.28.1",
    "pug": "^2.0.3",
    "ramda": "^0.26.1",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.2.0",
    "ttn": "^2.3.2",
    "uuid": "^3.3.2",
    "winston": "^3.1.0",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "codacy-coverage": "^3.4.0",
    "crypto-random-string": "^3.0.1",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "ip": "^1.1.5",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prisma": "^1.33.0",
    "speccy": "^0.11.0"
  },
  "engines": {
    "node": ">=8.3.0",
    "npm": ">=5"
  }
}
