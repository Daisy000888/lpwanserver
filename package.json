{
  "name": "lpwanserver-rest",
  "version": "1.0.1",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/rest",
    "package": "node ./bin/package.js",
    "test:api": "./bin/test/api",
    "test:e2e": "./bin/test/e2e",
    "test:e2e-https": "./bin/test/e2e-https",
    "test:unit": "./bin/test/unit",
    "test:all": "./bin/test/unit && ./bin/test/api && ./bin/test/e2e",
    "clean": "./bin/clean.js",
    "prisma": "cd prisma && prisma",
    "silent": "npm start > output.txt 2 > error.txt",
    "lint": "eslint rest/",
    "doc": "jsdoc -r ./rest -d ./docs",
    "coverage": "cat ./coverage/lcov.info | codacy-coverage",
    "openapi:lint": "speccy lint docs/openapi/api.yml",
    "openapi:publish": "speccy resolve -o docs/openapi/dist/api.yml docs/openapi/api.yml",
    "openapi:serve": "speccy serve docs/openapi/api.yml"
  },
  "dependencies": {
